{
  "name": "@aurabx/harmony-examples",
  "version": "0.11.0",
  "description": "Example pipeline configurations and templates for Harmony/Runbeam",
  "license": "MIT",
  "main": "./index.cjs",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.cjs"
    },
    "./pipelines.json": "./pipelines.json",
    "./transforms.json": "./transforms.json",
    "./workload-diagrams.json": "./workload-diagrams.json",
    "./pipelines/*": "./pipelines/*",
    "./transforms/*": "./transforms/*"
  },
  "files": [
    "pipelines.json",
    "transforms.json",
    "workload-diagrams.json",

    "pipelines/**/README.md",
    "pipelines/**/config.toml",
    "pipelines/**/pipelines/**/*.toml",
    "pipelines/**/transforms/**/*.json",
    "pipelines/**/*.sh",
    "pipelines/**/*.py",
    "pipelines/**/*.dcm",

    "transforms/**/*.json",

    "index.cjs",
    "index.mjs",
    "index.d.ts",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/aurabx/harmony-examples.git"
  },
  "bugs": {
    "url": "https://github.com/aurabx/harmony-examples/issues"
  },
  "homepage": "https://github.com/aurabx/harmony-examples#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "harmony",
    "runbeam",
    "pipelines",
    "examples",
    "fhir",
    "dicom",
    "dicomweb",
    "jolt"
  ]
}
